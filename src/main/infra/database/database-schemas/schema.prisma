datasource db_user {
    provider          = "postgresql"
    url               = env("POSTGRESQL_URI_PROD")
    shadowDatabaseUrl = env("POSTGRESQL_URI")
}

generator client {
  provider = "prisma-client-js"
}

model User {
    id       Int     @id @default(autoincrement())
    name     String  @db_user.VarChar(64)
    email    String  @unique @db_user.VarChar(64)
    imageUrl String? @db_user.VarChar(1024)
    password String  @db_user.VarChar(128)
}
